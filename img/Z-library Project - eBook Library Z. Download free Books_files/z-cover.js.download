class Cover extends HTMLElement {
    style(){
        return `
            :host{
                width: 100%;
                max-height: 100%;
                position: relative;
                display: block;
                // padding-top: 78.8%;
            }
            #div{
                width: 100%;
                height: 100%;
                aspect-ratio: 1 / 1.415;
                position: relative;
                opacity: 0;
                transition: opacity .3s ease;
            }
            #div.show{
                opacity: 1;
            }
            img{
                width: 100%;
                height: 100%;
                transition: opacity .2s ease;
                user-select: none;
                box-sizing: border-box;
                object-fit: contain;
                object-position: center;
                opacity: 0;
                ${this.hasAttribute('naturalratio') ? `position: relative;margin-bottom: -8px;` : `position: absolute`}
            }
            img.cover{
                object-fit: cover;
            }
            .bordered img{
                border: 4px solid #fff;
            }
            #div.size-100{
                width: 100px;
                height: 141px;
            }
            #div.bordered{
                border: 1px solid var(--gray-2);
                box-sizing: border-box;
                background: var(--card-bg-color);
            }
            // #div.booklist{
            //     width: 115px;
            //     height: 150px;
            // }
            .placeholder{
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                // opacity: 0;
                height: 100%;
                color: #61513b;
                transition: opacity .2s ease;
                overflow: hidden;
                box-sizing: border-box;
            }
            
            .placeholder .image{
                background: url(data:image/png;base64,);
                background-size: contain;
                background-repeat: no-repeat;
                background-position: right bottom;
                position: absolute;
                right: 0;
                bottom: 6%;
                left: 14%;
                top: 50%;
            }
            .placeholder{
                display: flex;
                flex-direction: column;
                justify-content: space-around;
            }
            .placeholder .clear{
                flex-grow: 1;
            }
            .placeholder .clearx3{
                flex-grow: 3.6;
            }
            .placeholder .title-wrap{
                background: #FFFFFF3D;
                padding: 5%;
            }
            .placeholder .title{
                font-size: 14px;
                line-height: 1.2em;
                text-align: left;
                white-space: initial;
                display: flex;
                justify-content: center;
                overflow: hidden;
                -webkit-line-clamp: 3;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-box-orient: vertical;
                text-shadow: 1px 1px 0px #FFFFFF44;
                text-transform: uppercase;
                font-weight: bold;
                word-break: keep-all;
            }
            .placeholder .author{
                font-size: 11px;
                line-height: 1.2em;
                text-align: left;
                padding: 0 0;
                white-space: initial;
                overflow: hidden;
                -webkit-line-clamp: 3;
                text-overflow: ellipsis;
                display: -webkit-box;
                -webkit-box-orient: vertical;
                max-height: 3.5em;
                padding-left: 5%;
                padding-right: 2%;
                margin-top: 4%;
                text-shadow: 1px 1px 0px #FFFFFF44;
            }
            
            .bordered .placeholder{
                width: auto; height: auto;
                left: 4px; right: 4px; top: 4px; bottom: 4px;
            }
            a{
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                box-shadow: 0 0 0 1px #00000000;
                transition: box-shadow var(--transition);
            }
            .bordered a:hover{
                transition: box-shadow var(--transition-hover);
                box-shadow: 0 0 0 1px var(--primary-color)
            }
            .mark{
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                box-shadow: 0 0 0 1px var(--blue-color), inset 0 0 0 1px var(--blue-color);
                opacity: 0;
                pointer-events: none;
            }
            .mark .label{
                max-width: 90%;
                position: absolute;
                bottom: 0;
                right: 0;
                background: var(--blue-color);
                padding: 2px 4px 0 6px;
                border-radius: 10px 0 0 0;
                color: #fff;
                font-size: 11px;
                line-height: 1.8;
                pointer-events: none;
                transition: opacity .2s ease;
            }
            .mark.downloaded{
                opacity: 1;
            }
            .mark-as-downloaded{
                position: absolute;
                right: 0;
                bottom: 0;
                background: #ccc;
                transition: background var(--transition);
                pointer-events: auto;
                cursor: pointer;
                opacity: 0;
                transition: opacity var(--transition);
                padding: 2px 4px 0 6px;
                border-radius: 10px 0 0 0;
                font-size: 11px;
                line-height: 1.8;
                z-index: 2;
                color: #fff;
            }
            .image:hover + .mark-as-downloaded{
                opacity: 1;
                transition: opacity var(--transition-hover);
            }
            .mark-as-downloaded:hover{
                opacity: 1;
                background: var(--blue-color);
                transition: background var(--transition-hover);
            }
            
            .volume .placeholder, .volume img{
                border-radius: 0 4px 3px 2px;
            }
            .volume .placeholder{
                padding: 0 2px 0 6px;
                border-bottom-right-radius: 8px;
            }
            .volume img{
                margin-bottom: -7px;
                border-bottom-right-radius: 8px;
            }
            .volume .volume{
                top: 0;
                left: 0;
                position: absolute;
                width: 100%;
                height: 100%;
                border-radius: 0 4px 3px 2px;
                // box-shadow: inset -2px -2px 0 #fff, inset -2px -2px 5px rgba(0,0,0,0.18), 0px 0px 2px rgba(0,0,0,0.4), 4px 4px 6px 4px #0000000F;
                box-shadow: inset -2px -2px 0 #fff, inset -2px -2px 5px rgba(0,0,0,0.18), 0px 0px 2px rgba(0,0,0,0.4);
                background-image: url("data:image/svg+xml,%0A%3Csvg width='6' height='150' viewBox='0 0 6 150' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Crect width='6' height='150' fill='url(%23paint0_linear)'/%3E%3Cdefs%3E%3ClinearGradient id='paint0_linear' x1='6' y1='61.5234' x2='-9.54301e-06' y2='61.5315' gradientUnits='userSpaceOnUse'%3E%3Cstop stop-color='white' stop-opacity='0'/%3E%3Cstop offset='0.139111' stop-color='white' stop-opacity='0.2'/%3E%3Cstop offset='0.290477' stop-opacity='0.18'/%3E%3Cstop offset='0.726819' stop-color='%23D8D8D8' stop-opacity='0.273181'/%3E%3Cstop offset='0.839352' stop-opacity='0.15'/%3E%3Cstop offset='1' stop-opacity='0.19'/%3E%3C/linearGradient%3E%3C/defs%3E%3C/svg%3E%0A");
                background-repeat: repeat-y;
                opacity: 1;
                transition: opacity .2s ease;
                pointer-events: none;
            }
            .texture{
                position: absolute;
                top:0;
                left:0;
                width:100%;
                height:100%;
                background-image: url(data:image/png;base64,);
                background-blend-mode: multiply;
                mix-blend-mode: multiply;
            }
            .blured-cover-wrap{
                position: absolute;
                top: 0;
                width: 100%;
                height: 100%;
                left: 0;
                overflow: hidden;
                opacity: 0;
                transition: opacity .2s ease;
            }
            .blured-cover{
                position: absolute;
                top: 0;
                width: 100%;
                height: 100%;
                left: 0;
                background-size: auto 105%;
                background-position: center;
                filter: blur(5px);
            }
            .volume .blured-cover-wrap{
                border-radius: 0 3px 8px 0;
            }
            .complete img, .complete .blured-cover-wrap{
                opacity: 1;
            }
            .complete .placeholder{
                opacity: 0;
            }
            :host([lightbox]) img{
                cursor: pointer;
            }
        `
    }
    palette() {return [["3596EE", "C2E9FF", "006D95"], ["258BB3", "8EDFFF", "006D95"], ["3569EE", "C2E9FF", "006D95"], ["395EAF", "C9D8FF", "006D95"], ["F17E74", "FFE0B5", "B65536"], ["E18D4B", "FEE592", "B65536"], ["DCB942", "FFEA9E", "B65536"], ["CA4E6B", "FFD9E4", "B448A1"], ["9C5A90", "FFDCED", "B448A1"], ["936ABD", "F2E1FF", "B549A2"], ["6964DE", "FFD8FA", "B549A2"], ["3C8782", "AAF5F0", "0D7D77"], ["149A5E", "C4F3D3", "0D7D77"], ["3E9A12", "CDF6D5", "0D7D77"]];}

    render() {
        const styles = [
            this.size ? 'size-' + this.size : '',
            this.bordered ? 'bordered' : '',
            this.volume ? 'volume' : '',
            this.source == 'booklist' ? 'booklist' : ''
        ];
        const linkNode = this.parentNode.tagName == 'A' ? this.parentNode : this.querySelector('a');
        return `
            <div class="${styles.join(' ')}" id="div" title="${ZLibrary.removeHTML(this.title)}">
                ${this.title ? this.makePlaceholder() : ``}
                <div class="blured-cover-wrap"><div class="blured-cover"></div></div><img class="image" onerror="this.classList.add('no-cover');" />
                ${this.markButton ? `<div class="mark-as-downloaded"><span class="label">${i18next.t('Mark as downloaded')}</span></div>` : ``}
                ${this.hasAttribute('volume') ? `<div class="volume"></div>` : ``}
                <span class="mark ${this.downloaded ? 'downloaded' : ''}"><span class="label">${i18next.t('Downloaded')}</span></span>
                ${linkNode ? `<a href="${linkNode.getAttribute('href')}" data-target-desktop="new-tab" target="${linkNode.hasAttribute('target') ? linkNode.getAttribute('target') : `_self`}"></a>` : ``}
            </div>
        `
    }
    makePlaceholder() {
        let palette = this.palette()
        this.palettePointer = this.id ? this.id.substring(this.id.length - 2) % palette.length : Math.floor(Math.random() * (palette.length - 1))
        return `
            <div class="placeholder" style="background: linear-gradient(313deg, #${palette[this.palettePointer][0]} 0%, #${palette[this.palettePointer][1]} 100%);">
                ${!this.stackMode ? `<div class="image"></div><div class="texture"></div>` : ``}
                <div class="clear"></div>
                <div class="title-wrap"><div class="title" style="color:#${palette[this.palettePointer][2]}">${ZLibrary.removeHTML(this.title)}</div></div>
                <div class="author" style="color:#${palette[this.palettePointer][2]}">${ZLibrary.removeHTML(this.author)}</div><div class="clearx3"></div>
            </div>
        `
    }
    showLightbox(){
        const sourceImage = this.shadowRoot.querySelector('img')
        if(!sourceImage) return
        const URL = sourceImage.hasAttribute('data-src') ? sourceImage.getAttribute('data-src') : sourceImage.getAttribute('src')
        const re = /covers[^/]*\//;
        const orignalImageURL = URL.replace(re, "covers1000/");
        window.ZLibrary.DOM.lightbox = orignalImageURL
    }
    connectedCallback() {
        if (this.shadowRoot) return
        const shadow = this.attachShadow({ mode: 'open' });
        this.id = this.hasAttribute('id') ? this.getAttribute('id') : '';
        this.isbn = this.hasAttribute('isbn') ? this.getAttribute('isbn').split(',') : [];
        this.title = this.getAttribute('title');
        this.size = this.getAttribute('size');
        this.bordered = this.hasAttribute('bordered') && !this.hasAttribute('volume');
        this.volume = this.hasAttribute('volume');
        this.source = this.getAttribute('source');
        this.width = this.getAttribute('width');
        this.author = this.getAttribute('author');
        this.markButton = this.hasAttribute('markButton');
        // this.stackMode = this.querySelector('img')?.hasAttribute('stack-mode');
        this.downloaded = ZLibrary.checkIsDownloaded(this.getAttribute('id'), ...this.isbn);
        shadow.innerHTML = '<style>' + this.style() + '</style>' + this.render();
        this.root = this.shadowRoot.getElementById('div');

        const viewportIntersectionObserver = new IntersectionObserver((entries, observer) => {
            entries.forEach(entry => {
                if (entry.isIntersecting && !this.root.classList.contains('show')) {
                    this.root.classList.add('show')
                    this.titleSizeCheck()
                    let sourceImage = this.querySelector('img')
                    if(!sourceImage) return
                    let shadowImage = this.shadowRoot.querySelector('img')
                    let range = sourceImage.hasAttribute('src') ? 1.25 : 1 // allow worst quality for SEO-optimazed images or strict mode for 'data-src'
                    const URL = sourceImage.hasAttribute('data-src') ? sourceImage.getAttribute('data-src') : sourceImage.getAttribute('src')

                    if(URL && URL !=='null' && URL !=='undefined' && URL.substring(0,5) !== '/img/'){
                        let initialsize = URL.split('/')[3].split('covers')[1]

                        var devicePixelRatio = window.devicePixelRatio;
                        let coversize = initialsize
                        let offsetWidth = this.offsetWidth
                        if(offsetWidth === 0){
                            offsetWidth = this.parentNode.offsetWidth
                        }
                        if (offsetWidth <= 400 * range) { if (devicePixelRatio > 1.5) coversize = 400; if (devicePixelRatio <= 1.5) coversize = 400; if (devicePixelRatio <= 1) coversize = 400; }
                        if (offsetWidth <= 300 * range) { if (devicePixelRatio > 1.5) coversize = 400; if (devicePixelRatio <= 1.5) coversize = 400; if (devicePixelRatio <= 1) coversize = 300; }
                        if (offsetWidth <= 200 * range) { if (devicePixelRatio > 1.5) coversize = 400; if (devicePixelRatio <= 1.5) coversize = 300; if (devicePixelRatio <= 1) coversize = 200; }
                        if (offsetWidth <= 150 * range) { if (devicePixelRatio > 1.5) coversize = 300; if (devicePixelRatio <= 1.5) coversize = 200; if (devicePixelRatio <= 1) coversize = 150; }
                        if (offsetWidth <= 100 * range) { if (devicePixelRatio > 1.5) coversize = 200; if (devicePixelRatio <= 1.5) coversize = 150; if (devicePixelRatio <= 1) coversize = 100; }
                        if(sourceImage.hasAttribute('src') && initialsize > coversize){ // prevent download worst size in SEO-mode
                            coversize = initialsize
                        }
                        if (this.shadowRoot.querySelector('.blured-cover')) {
                            this.shadowRoot.querySelector('.blured-cover').style.backgroundImage = 'url(' + URL.replace('covers' + initialsize, 'covers' + coversize) + ')'
                            this.shadowRoot.querySelector('.blured-cover').style.opacity = 1
                        }
                        shadowImage.setAttribute('src', URL.replace('covers' + initialsize, 'covers' + coversize))
                        this.shadowRoot.querySelector('img').classList.remove('no-cover')
                        if(this.hasAttribute('lightbox')){
                            shadowImage.addEventListener('click', ()=>{
                                this.showLightbox()
                            })
                        }
                    }
                    sourceImage.removeAttribute('data-src')
                    sourceImage.removeAttribute('src')
                    // observer.unobserve(entry.target);
                }
            });
        });
        /* listen if downloaded */
        document.addEventListener('marksLoaded', () => {
            this.downloaded = ZLibrary.checkIsDownloaded(this.getAttribute('id'), ...this.isbn);
            if (this.downloaded) {
                this.shadowRoot.querySelector('.mark-as-downloaded')?.remove()
                this.shadowRoot.querySelector('.mark').classList.add('downloaded')
            }
        })
        if (this.markButton && this.id) {
            this.shadowRoot.querySelector('.mark-as-downloaded .label').addEventListener('click', () => {
                CurrentUser.addDownloadedBook(this.id)
                new ZLibraryResponse(`/papi/user/count-download/${this.id}`)
                    .fetch()
            })
        }
        // load callback
        if (this.shadowRoot.querySelector('img')) {
            const imageElement = this.shadowRoot.querySelector('img');
            imageElement.addEventListener('load', () => {
                if (imageElement.naturalWidth / imageElement.naturalHeight < imageElement.offsetWidth / imageElement.offsetHeight) {
                    imageElement.classList.add('cover');
                    this.shadowRoot.querySelector('.blured-cover-wrap')?.remove();
                }
                this.root.classList.add('complete');
                if (this.hasAttribute('naturalratio')) {
                    this.root.style.aspectRatio = 'auto';
                }
            })
        }
        /* stack-mode is special mode for tiled booklists to prevent viewportIntersection */
        if(!this.querySelector('img')?.hasAttribute('stack-mode')){
            setTimeout(()=>{
                viewportIntersectionObserver.observe(this);
            }, 300)
        }else{
            const observer = new MutationObserver((mutationsList, observer) => {
                if(!this.shadowRoot.querySelector('img')?.hasAttribute('stack-mode')){
                    viewportIntersectionObserver.observe(this);
                }
            });
            observer.observe(this.querySelector('img'), { attributes: true });
        }
        ZLibrary.dispatch('ready', this)
    }
    titleSizeCheck() {
        if(this.shadowRoot.querySelector('.placeholder')){
            let sizes = [11,11];
            if (this.offsetWidth > 170) {sizes = [16,12];}
            if (this.offsetWidth > 200) {sizes = [18,13];}
            if (this.offsetWidth > 220) {sizes = [20,13];}
            if (this.offsetWidth < 110) {sizes = [12,10];}
            if (this.offsetWidth < 70) {
                this.shadowRoot.querySelector('.placeholder .image').style.display = 'none'
            }
            if(this.title.length < 20){sizes[0]*=1.2}
            if(this.title.length < 12){sizes[0]*=1.4}
            this.shadowRoot.querySelector('.title').style.fontSize = sizes[0] + 'px'
            this.shadowRoot.querySelector('.author').style.fontSize = sizes[1] + 'px'
            ZLibrary.fontAdaptor(this.shadowRoot.querySelector('.title'))
            ZLibrary.fontAdaptor(this.shadowRoot.querySelector('.author'))
            ZLibrary.fontAdaptor(this.shadowRoot.querySelector('.mark .label'), 8)
        }
    }
    disconnect() {
        this.remove();
    }
}
customElements.define('z-cover', Cover)
